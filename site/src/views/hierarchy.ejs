    <link rel="stylesheet" href="/css/colors.css">
</head>

<body>

    <div class="box-guilds">
        <div class="guild">
            <a href="/" class="logo">
                <img src="/img/logo-ds-128x128.png" alt="Matrix Roleplay">
            </a>
            <div class="name">
                Matrix Roleplay (Zion)
            </div>
            <div class="selected"></div>
        </div>
    </div>

    <div class="box-content no-channels">
        <div class="inner">
            <div class="title">
                ðŸ’¥ÊœÉªá´‡Ê€á´€Ê€qá´œÉªá´€
            </div>
            <div class="body" id="regras-markdown">

                <% for (var group_id in groups) {  %>
                <div class="markdown">
                    <h3><%- groups[group_id].name %></h3>
                </div>

                <div class="table-responsive">
                    <table class="table table-bordered table-striped table-discord">
                        <tbody>
                            <tr>
                                <td width="200"></td>

                                <% for (var squad_id in groups[group_id].squads) {  %>

                                <th width="250">
                                    <%- groups[group_id].squads[squad_id].name %>
                                </th>

                                <% } %>

                                <td></td>
                            </tr>

                            <% for (var level_id in groups[group_id].levels) {  %>

                            <tr>
                                <th>

                                    <%- groups[group_id].levels[level_id].name %>

                                    <% if (groups[group_id].levels[level_id].salary > 0) { %>

                                    <br>
                                    <small class="text-logo-400">$
                                        <%- groups[group_id].levels[level_id].salaryFormatted %>
                                    </small>

                                    <% } %>

                                </th>

                                <% for (var squad_id in groups[group_id].squads) {  %>

                                <td>

                                    <% if (groups[group_id].squads[squad_id].levels[level_id])  { %>
                                    <% for (var member_id in groups[group_id].squads[squad_id].levels[level_id].members) {  %>

                                    <p>
                                        <%- groups[group_id].squads[squad_id].levels[level_id].members[member_id].fullname %>
                                    </p>

                                    <% } %>
                                    <% } %>

                                </td>

                                <% } %>

                                <td></td>

                            </tr>

                            <% } %>
                        </tbody>
                    </table>
                </div>

                <% } %>

            </div>
        </div>
    </div>

    <script src="https://cdnjs.cloudflare.com/ajax/libs/jquery/3.5.1/jquery.min.js"
        integrity="sha512-bLT0Qm9VnAYZDflyKcBaQ2gg0hSYNQrJ8RilYldYQ1FxQYoCLtUjuuRuZo+fjqhx/qtq/1itJ0C2ejDxltZVFg=="
        crossorigin="anonymous"></script>

    <script src="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/js/bootstrap.min.js"
        integrity="sha384-Tc5IQib027qvyjSMfHjOMaLkfuWVxZxUPnCJA7l2mCWNIpG9mGCD8wGNIcPD7Txa"
        crossorigin="anonymous"></script>

</body>

